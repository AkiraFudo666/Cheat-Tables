-- createScripts("https://cdn.discordapp.com/attachments/350754184267694081/1158988146432811109/testScript.lua", "My Test Script", 0x000080, "Script Header Name", true, false)
function createScripts(scriptCode, scriptName, scriptColor, scriptParent, hideChildren, manualExpandCollapse)
   local mr = AddressList.createMemoryRecord()
   local al = AddressList
   mr.script = getInternet().getURL(scriptCode) -- getInternet().getURL("https://cdn.discordapp.com/attachments/350754184267694081/1158988146432811109/testScript.lua")
   mr.description = scriptName
   mr.varType = vtAutoAssembler
   mr.color = scriptColor
   if scriptParent ~= false then
      mr.Parent = al.getMemoryRecordByDescription(scriptParent)
   end
   if hideChildren == true and manualExpandCollapse == true then
      mr.Options = '[moDeactivateChildrenAsWell, moHideChildren, moManualExpandCollapse]'
   elseif hideChildren == true and manualExpandCollapse == false then
      mr.Options = '[moDeactivateChildrenAsWell, moHideChildren]'
   elseif hideChildren == false and manualExpandCollapse == true then
      mr.Options = '[moDeactivateChildrenAsWell, moManualExpandCollapse]'
   else
      mr.Options = '[moDeactivateChildrenAsWell]'
   end
end

-- Get Static Field Values
function getFValues(className,offsetName,nameSpace)
   if not nameSpace then nameSpace = '' end
   local cid = mono_findClass(nameSpace,className)
   local fields = mono_class_enumFields(cid,true)
   for i = 1,#fields do
       if fields[i].name == offsetName then
          return mono_class_getStaticFieldValue(cid,fields[i].field)
       end
   end
end
