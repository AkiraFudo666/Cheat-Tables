-- Get Static Field Address (Mono)
function writeUserSymbols(classname, fieldname,symbolname)
   local cid       = mono_findClass('',classname)
   local theFields = mono_class_enumFields(cid,true)
   local sf
   for i=1,#theFields do
       if theFields[i].isStatic and theFields[i].name == fieldname then
          sf = mono_class_getStaticFieldAddress('',cid)
          sf = sf + theFields[i].offset
          registerSymbol(symbolname,getAddress(sf), true)
       end
   end
end
--writeUserSymbols('Game','isModded','isModded_Ptr')

-- Get Static Field Address (IL2CPP)
function getExStaticFieldAddress(className, OffsetName, nameSpace)
   if not nameSpace then nameSpace = '' end
   local cid = mono_findClass(nameSpace, className)
   local fields = mono_class_enumFields(cid, true)
   for i = 1,#fields do
       if fields[i].name == OffsetName then
          return readPointer(cid+0xB8) + fields[i].offset
       end
   end
end
